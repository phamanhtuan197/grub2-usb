title 5. Gparted Live CD i486
    	find --set-root /iso/gparted-live-i486.iso
    	map /iso/gparted-live-0.11.0-10.iso (0xff) || map --mem /iso/gparted-live-i486.iso (0xff)
    	map --hook
    	root (0xff)
    	kernel /live/vmlinuz  boot=live config union=aufs noswap noprompt vga=788 ip=frommedia findiso=/iso/gparted-live-i486.iso toram=filesystem.squashfs
    	initrd /live/initrd.img

