gfxmenu /GFXBoot.gfx

title 1. Ubuntu LiveCD Destop amd64
	find --set-root /iso/ubuntu-desktop-amd64.iso
	map /iso/ubuntu-desktop-amd64.iso (0xff)
	map --hook
	root (0xff)
	kernel /casper/vmlinuz.efi boot=casper iso-scan/filename=/iso/ubuntu-desktop-amd64.iso quiet splash
	initrd /casper/initrd.lz
	boot

title 2. Ubuntu Install Desktop amd64
	find --set-root /iso/ubuntu-desktop-amd64.iso
	map /iso/ubuntu-desktop-amd64.iso (0xff)
	map --hook
	root (0xff)
	kernel /casper/vmlinuz.efi boot=casper only-ubiquity iso-scan/filename=/iso/ubuntu-desktop-amd64.iso quiet splash
	initrd /casper/initrd.lz
	boot

title 3. Ubuntu LiveCD Desktop i386
	find --set-root /iso/ubuntu-desktop-i386.iso
	map /iso/ubuntu-desktop-i386.iso (0xff) || map --mem /iso/ubuntu-desktop-i386.iso (0xff)
	map --hook
	root (0xff)
	kernel /casper/vmlinuz   boot=casper iso-scan/filename=/iso/ubuntu-desktop-i386.iso quiet splash --
	initrd /casper/initrd.lz

title 4. Ubuntu Install Desktop i386
	find --set-root /iso/ubuntu-desktop-i386.iso
